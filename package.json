{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "node ./scripts/run-eslint.mjs .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "nuxt typecheck",
    "vercel:ignore": "node scripts/vercel-ignore-build.js",
    "vercel:ignore:bash": "bash scripts/vercel-ignore-build.sh",
    "ci:check": "pnpm lint && pnpm test:run && pnpm typecheck"
  },
  "dependencies": {
    "@nuxt/content": "3.7.0",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.11.13",
    "@nuxt/test-utils": "3.19.2",
    "@nuxt/ui": "3.3.3",
    "@unhead/vue": "^2.0.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@vueuse/core": "^13.9.0",
    "better-auth": "^1.3.27",
    "better-sqlite3": "^11.5.0",
    "eslint": "^9.0.0",
    "mongoose": "^8.19.1",
    "nuxt": "^4.1.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.6.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "@vercel/speed-insights",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.0",
    "@iconify-json/mdi": "^1.2.0",
    "@types/node": "^24.5.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "happy-dom": "^20.0.1",
    "nodemailer": "^7.0.6",
    "nuxt-nodemailer": "^1.1.2",
    "pug": "^3.0.3",
    "vitest": "^3.2.4"
  }
}
